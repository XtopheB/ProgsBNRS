# ---------------------------------------------------

Nboot<-9
Fips.P <- matrix(999, nrow=3, ncol=3, 
                dimnames = list(c("1", "2", "3")))
                
Fips.P[1,2] <- TestDistSimu(eff3.1,eff3.12, nboot = Nboot)$Pval
